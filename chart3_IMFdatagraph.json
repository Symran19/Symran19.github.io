{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Total IMF Credit Outstanding: 1984 - 2022 ",
    "subtitle": "Yearly lending in billions. Source: IMF",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/Symran19/Symran19.github.io/main/IMF_data_OutstandingCleanNew.csv"
  },
  "transform": [{"calculate": "datum.Totals/1000000000", "as": "Billions"}],
  "height": 300,
  "width": 350,
  "mark": {"type": "bar", "color": "#86d0bb"},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "Billions",
      "type": "quantitative",
      "title": "",
      "axis": {"grid": false}
    }
  },
  "config": {}
}
